      default:
        return false; ///new Response(JSON.stringify({ message: "Route not found", code: 404 }), { status: 404 });
    }
  } catch (error) {
    return new Response(JSON.stringify({ message: `Internal server error: ${error}`, code: 500 }), {
      status: 500,
    });
  }
}

