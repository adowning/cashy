  subscribe: AppEventsSubscription | '*'
  onConnect?: (data: CashflowInfo) => void
  onDisconnect?: () => void
  onError?: (error: Error) => void
  onData?: (data: any) => void
}

export const DefaultCashflowClientOptions: CashflowClientOptions = {
  scheme: 'ws',
  host: '127.0.0.1',
  port: 3000,
  endpoint: '/api/setup',
  immediate: true,
  autoReconnect: true,
  retries: -1,
  subscribe: {},
} as const

