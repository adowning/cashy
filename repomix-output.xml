// Array of keys
export function pick<T extends object, U extends Extract<keyof T, string>>(
  obj: T,
  paths: U[],
): MaybePick<T, U> {
  const found: any = {}

  for (const key of paths) {
    if (Object.prototype.hasOwnProperty.call(obj, key)) {
      found[key] = obj[key]
    }
  }

  return found
}

