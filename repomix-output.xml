  export function convertToUnit (str: string | number | null | undefined, unit = 'px'): string | undefined {
    if (str == null || str === '') {
      return undefined
    }
    const num = Number(str)
    if (isNaN(num)) {
      return String(str)
    } else if (!isFinite(num)) {
      return undefined
    } else {
      return `${num}${unit}`
    }
  }

