<script setup>
import ComponentLastWinner from './LastWinner.vue'
import ComponentSpiner from './Spinner.vue'
import ComponentCongrats from './Congrats.vue'
import HttpApi from '@/Services/HttpApi.js'
import { reactive, ref, computed, onMounted } from 'vue'

// Data
const showWinList = ref(false)
const prize = ref(null)
const total_bonus = reactive(window.totalBonus)
const lastWinner = reactive({
  avatar: '/assets/images/spin/avatar.png',
  name: '',
  prize: 0,
  currency: '',
})
const lastWinners = reactive([])
const winnerList = reactive([])
const totalBonus = ref(0)
const interation = ref(0)

// Props
const props = defineProps(['title', 'content', 'showButtons', 'showModal', 'winners'])

// Computed

// Methods
const closeModal = function () {
  this.$emit('close')
}

const PrizedHandle = function (v) {
  prize.value = v
}

const OpenWinList = function () {
  showWinList.value = true
  this.$emit('open-win-list', showWinList.value)
}

const HideWinList = function () {
  showWinList.value = false
}

const GetWinners = async function () {
  const vm = this
  await HttpApi.get('spin/winners?sessionId=2', {
    action: 'get-winners',
  }).then((resp) => {
    vm.winnerList = resp.data[0].map((w) => {
      w.prize = Number(w.prize).toFixed(2).replace('.', ',')
      return w
    })
    vm.totalBonus = resp.data[1]

    vm.loopWinner()
  })
}

const loopWinner = function () {
  const interation = interation.value

  const lastWinner = winnerList[interation]

  lastWinners.push(lastWinner)

  const vm = this

  interation.value++
  if (interation.value === lastWinners.length) {
    interation.value = [0]
  }

  if (lastWinners.length > 1) {
    setTimeout(() => {
      const allEl = document.querySelectorAll('#r-c-container > div')
      const first = allEl[0]
      const secound = allEl[1]

      const interval = setInterval(() => {
        const fX = Number(first.style.top.split('px')[0])
        const sX = Number(secound.style.top.split('px')[0])
        const tilt = -fX * 0.5

        first.style.transform = `perspective(100px) rotateX(${tilt}deg)`
        secound.style.transform = `perspective(100px) rotateX(${-(tilt / 12)}deg)`

        first.style.top = ''.concat(fX - 5, 'px')
        secound.style.top = ''.concat(sX - 5, 'px')

        if (sX < -92) {
          first.remove()
          clearInterval(interval)
          secound.style.transform = `perspective(100px) rotateX(0deg) rotateY(0deg)`
          secound.style.top = '0px'
          setTimeout(() => {
            vm.loopWinner()
          }, 1000)
        }
      }, 50)
    }, 1000)
  } else {
    setTimeout(() => {
      vm.loopWinner()
    }, 3000)
  }
}

const HandleLoaded = function () {
  this.$emit('loaded')
}

// Mounted
onMounted(() => {
  ;(async () => {
    await GetWinners()
  })()
})
</script>

