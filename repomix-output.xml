watch(socketBalance, (value) => {
  console.log('socketBalance================', value)
  let locale = 'pt-BR'
  switch (value.cur) {
    case 'BRL':
      locale = 'pt-BR'
      break
    case 'PHP':
      locale = 'en-PH'
      break
    case 'PEN':
      locale = 'en-PE'
      break
    case 'MXN':
      locale = 'es-MX'
      break
    case 'CLP':
      locale = 'es-CL'
      break
    case 'USD':
      locale = 'en-US'
    case 'COP':
      locale = 'es-CO'
      break
  }
  if (user.value.currency == value.cur) {
