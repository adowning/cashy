<script>
import { computed, ref, onMounted, onBeforeUnmount, watch } from 'vue'
// import { GRADIENT, SHADOW, BG_SHADOW } from '@/default'
export const GRADIENT = {
  angle: 0,
  startColor: '#ff0000',
  stopColor: '#ffff00',
}

export const SHADOW = {
  inset: false,
  vertical: 10,
  horizontal: 0,
  blur: 10,
  opacity: 0.5,
  color: '#000000',
}

export const BG_SHADOW = {
  inset: true,
  vertical: 3,
  horizontal: 0,
  blur: 3,
  opacity: 0.4,
  color: '#000000',
}

function uuid(prefix = '', suffix = '') {
  return (
    prefix +
    Math.random().toString(36).substring(2, 8) +
    Math.random().toString(36).substring(2, 8) +
    suffix
  )
}

export default {
  name: 'circle-progress',
  props: {
    size: {
      type: Number,
      default: 180,
    },
    borderWidth: {
      type: Number,
      default: 15,
    },
    borderBgWidth: {
      type: Number,
      default: 15,
    },
    fillColor: {
