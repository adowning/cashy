    updateDepositRate(amount)
  } else {
    depositRate.value = 0 // Ensure rate is 0 if not participating
  }

  const isValid = validateAmount(amount)
  isShowAmountValidation.value = !isValid
  isDepositBtnReady.value = isValid
})

