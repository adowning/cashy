  // --- Create Chat Rooms (Modified to link to existing Game Sessions) ---
  const chatRoomsCount = getRandomInt(5, 15);
  const chatRooms = [];
  for (let i = 0; i < chatRoomsCount; i++) {
    const isGameRoom = faker.datatype.boolean();
