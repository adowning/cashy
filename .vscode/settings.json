{
  "todoHighlighter.exclude": [
    "node_modules/**",
    "dist/**",
    "coverage/**"
  ],
  "todoHighlighter.include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "bun.runtime": "/home/ash/.bun/bin/bun",
  // If support for Bun should be added to the default "JavaScript Debug Terminal".
  "bun.debugTerminal.enabled": true,
  // If the debugger should stop on the first line of the program.
  "bun.debugTerminal.stopOnEntry": false,
  // Glob pattern to find test files. Defaults to the value shown below.
  "bun.test.filePattern": "**/*{.test.,.spec.,_test_,_spec_}{js,ts,tsx,jsx,mts,cts}",
  "Codegeex.Chat.LanguagePreference": "English",
  "Codegeex.SidebarUI.LanguagePreference": "English",
  "Codegeex.Comment.LanguagePreference": "English",
  "Codegeex.CommitMessage.LanguagePreference": "English",
  "postman.settings.dotenv-detection-notification-visibility": false,
  "files.exclude": {
    ".editorconfig": false,
    ".env": false,
    ".gitignore": false,
    ".prettierrc.json": false,
    "**/node_modules/**": false,
    "bun.lock": false,
    "bunfig.toml": false,
    "client/.editorconfig": false,
    "client/.env": false,
    "client/.env.development": false,
    "client/.env.production": false,
    "client/.env.staging": false,
    "client/.eslintrc-auto-import.json": false,
    "client/.gitattributes": false,
    "client/.gitignore": false,
    "client/.prettierrc.json": false,
    "client/bun.lock": false,
    "client/env.d.ts": false,
    "client/eslint.config.js": false,
    "client/node_modules": false,
    "client/openapi-ts.config.ts": false,
    "client/sdk-generator.json": false,
    "client/swagger.json": false,
    "client/tailwind.config.js": false,
    "client/tsconfig.client.json": false,
    "client/tsconfig.node.json": false,
    "logo.png": false,
    "package.json": false,
    "prisma/enum-generator.ts": false,
    "README.md": false,
    "server/tsconfig.server.json": false,
    "tools": false,
    "tsconfig.json": false
  },
  "files.watcherExclude": {
    "**/.wrangler/**": true,
    "**/datadir/**": true,
    "**/node_modules/**": true,
    "**/old/**": true,
    "**/prisma/client/**": true,
    "**/public/**": true
  },
  "editor.fontFamily": "'JetBrains Mono', 'monospace', monospace",
  "editor.wordWrap": "on",
}