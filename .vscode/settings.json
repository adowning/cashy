{
  "todoHighlighter.exclude": [
    "node_modules/**",
    "dist/**",
    "coverage/**"
  ],
  "todoHighlighter.include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "bun.runtime": "/home/ash/.bun/bin/bun",
  // If support for Bun should be added to the default "JavaScript Debug Terminal".
  "bun.debugTerminal.enabled": true,
  // If the debugger should stop on the first line of the program.
  "bun.debugTerminal.stopOnEntry": false,
  // Glob pattern to find test files. Defaults to the value shown below.
  "bun.test.filePattern": "**/*{.test.,.spec.,_test_,_spec_}{js,ts,tsx,jsx,mts,cts}",
  "Codegeex.Chat.LanguagePreference": "English",
  "Codegeex.SidebarUI.LanguagePreference": "English",
  "Codegeex.Comment.LanguagePreference": "English",
  "Codegeex.CommitMessage.LanguagePreference": "English",
  "postman.settings.dotenv-detection-notification-visibility": false,
  "files.exclude": {
    "client/.editorconfig": true,
    "client/.eslintrc-auto-import.json": true,
    "client/.gitignore": true,
    "client/.gitattributes": true,
    "client/.prettierrc.json": true,
    "client/bun.lock": true,
    "client/env.d.ts": true,
    "client/eslint.config.js": true,
    "client/openapi-ts.config.ts": true,
    "client/sdk-generator.json": true,
    "client/swagger.json": true,
    "client/tailwind.config.js": true,
    "client/tsconfig.client.json": true,
    "client/tsconfig.node.json": true,
    "client/.env.staging": true,
    "client/.env.production": true,
    "client/.env.development": true,
    "client/.env": true,
    "client/node_modules": true,
    "prisma/enum-generator.ts": true,
    "server/tsconfig.server.json": true,
    "tsconfig.json": true,
    "README.md": true,
    "package.json": true,
    "logo.png": true,
    "bunfig.toml": true,
    "bun.lock": true,
    ".prettierrc.json": true,
    ".gitignore": true,
    ".editorconfig": true,
    ".env": true,
    "tools": true
  },
}
