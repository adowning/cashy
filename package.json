{
  "$schema": "https://json.schemastore.org/package",
  "name": "cashflow",
  "version": "0.2.1",
  "engines": {
    "bun": ">1.1.1"
  },
  "scripts": {
    "build": "npm run test:cov-build && npm run format:fix && npm run build:cjs && npm run build:esm && npm run build:types",
    "forcebuild": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "rimraf ./dist/cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "rimraf ./dist/esm && tsc -p config/tsconfig.esm.json",
    "build:types": "rimraf ./dist/types && tsc -p config/tsconfig.types.json",
    "clean": "rimraf ./dist/cjs ./dist/esm ./dist/umd ./dist/types",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "test:cov-browser": "jest --coverage --no-cache --runInBand && start ./coverage/lcov-report/index.html",
    "test:cov-build": "jest --coverage --no-cache --runInBand && jest-coverage-badges-ts --output \"./.coverage-badges\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "release": "rimraf ./*.tgz -g && npm run build && node tools/versionManager.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "deve": "concurrently --color -n \"SERVER,CLIENT,EMAIL\" -c \"green,magenta,cyan\" \"bun run --watch --filter server --elide-lines=1000 dev \" \"bun run --watch --filter client --elide-lines=1000  dev \" \"bun run --watch --filter cashapp-email --elide-lines=1000  dev \"",
    "dev": "concurrently --color -n \"SERVER,CLIENT\" -c \"green,magenta\" \"bun run --watch --filter server --elide-lines=1000 dev \" \"bun run --watch --filter client --elide-lines=1000  dev \""
  },
  "keywords": [
    "websocket",
    "server",
    "connection",
    "library",
    "eventhandler",
    "waiter",
    "async",
    "tcp",
    "realtime",
    "node",
    "typescript",
    "events"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.2.12",
    "@types/cacheable-request": "^8.3.6",
    "@types/cookie": "^1.0.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/http-cache-semantics": "^4.0.4",
    "@types/js-cookie": "^3.0.6",
    "@types/json-schema": "^7.0.15",
    "@types/json5": "^2.2.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/keyv": "^4.2.0",
    "@types/lodash": "^4.17.16",
    "@types/mdast": "^4.0.4",
    "@types/ms": "^2.1.0",
    "@types/normalize-package-data": "^2.4.4",
    "@types/resolve": "^1.20.6",
    "@types/responselike": "^1.0.3",
    "@types/slice-ansi": "^7.1.0",
    "@types/unist": "^3.0.3",
    "@types/uuid": "^10.0.0",
    "@types/web-bluetooth": "^0.0.21",
    "@types/ws": "^8.18.1",
    "bun-types": "^1.2.12",
    "prisma": "^6.7.0",
    "prisma-class-generator": "^0.2.11",
    "prisma-generator-typescript-interfaces": "^2.1.0",
    "zod-prisma": "^0.5.4"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@prisma/client": "^6.7.0",
    "bcryptjs": "^3.0.2"
  },
  "workspaces": [
    "client",
    "server",
    "shared"
  ]
}
